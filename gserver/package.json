  "author": "Alexander Krechik",
  "engines": {
    "node": "*"
  },
  "dependencies": {
    "glob-fs": "^0.1.6",
    "vscode-languageserver": "^2.6.2"
  },
  "devDependencies": {
    "@types/node": "^6.0.52",
    "tslint": "^4.2.0",
